import java.util.*;
public class Ejercicio1{
	public static void main(String[] args){
		Scanner in=new Scanner(System.in);
		
		int n=in.nextInt();
		List<Integer> nums=new ArrayList<>();
		for(int i=0;i<n;i++){
			nums.add(in.nextInt());
		}
		Collections.sort(nums,Collections.reverseOrder());
		
		System.out.println(nums);
	}

}